            "text": "<!DOCTYPE html>\n<html>\n<head>\n    <meta charset=\"UTF-8\">\n    <title>Request Variations Test</title>\n</head>\n<body>\n<h1>Test Request Variations</h1>\n\n        <form id=\"myForm\" action=\"multipart123\">\n            <input id=\"name\" type=\"text\" value=\"1\" />\n            <input id=\"data\" type=\"file\" />\n            <input id=\"name2\" type=\"text\" value=\"15\" />\n            <button type=\"submit\" name=\"submit\" />\n            <button onclick=\"uploader()\">Click</button>\n        </form>\n\n\t\t<form action=\"/login\" method=\"get\">\n\t\t\t<label for=\"user\">user:</label>\n\t\t\t<input type=\"text\" id=\"user\" name=\"user\"><br><br>\n\t\t\t<label for=\"password\">password:</label>\n\t\t\t<input type=\"password\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"submit\" value=\"Submit\">\n\t\t</form>\n\t\t<hr>\n\t\t<form action=\"/login-wo-type\">\n\t\t\t<label for=\"user\">user:</label>\n\t\t\t<input id=\"user\" name=\"user\"><br><br>\n\t\t\t<label for=\"PaSsWoRd\">password:</label>\n\t\t\t<input type=\"PaSsWoRd\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"Submit\" value=\"Submit\">\n\t\t</form>\n\t\t<hr>\n\t\t<form action=\"/login-with-email\">\n\t\t\t<label for=\"email\">email:</label>\n\t\t\t<input type=\"email\" id=\"email\" name=\"email\"><br><br>\n\t\t\t<label for=\"password\">password:</label>\n\t\t\t<input type=\"password\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"submit\" value=\"Submit\">\n\t\t</form>\n\t\t<hr>\n\t\t<form action=\"/login-with-search-type\">\n\t\t\t<label for=\"user\">user:</label>\n\t\t\t<input type=\"search\" id=\"user\" name=\"user\"><br><br>\n\t\t\t<label for=\"password\">password:</label>\n\t\t\t<input type=\"password\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"submit\" value=\"Submit\">\n\t\t</form>\n\t\t<hr>\n\t\t<form action=\"/register\">\n\t\t\t<label for=\"email\">email:</label>\n\t\t\t<input type=\"email\" id=\"email\" name=\"email\"><br><br>\n\t\t\t<label for=\"user\">user:</label>\n\t\t\t<input type=\"text\" id=\"user\" name=\"user\"><br><br>\n\t\t\t<label for=\"password\">password:</label>\n\t\t\t<input type=\"password\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"submit\" value=\"Submit\">\n\t\t</form>\n\t\t<hr>\n\t\t<form action=\"/contact\">\n\t\t\t<label for=\"email\">email:</label>\n\t\t\t<input type=\"email\" id=\"email\" name=\"email\"><br><br>\n\t\t\t<label for=\"name\">name:</label>\n\t\t\t<input type=\"text\" id=\"name\" name=\"name\"><br><br>\n\t\t\t<label for=\"attachment\">attachment:</label>\n\t\t\t<input type=\"file\" id=\"attachment\" name=\"attachment\"><br><br>\n\t\t\t<label for=\"message\">message:</label>\n\t\t\t<textarea id=\"message\" name=\"message\" rows=\"4\" cols=\"50\">\n\t\t\tYour message here\n\t\t\t</textarea>\n            <input type=\"submit\" value=\"Submit\">\n\t\t</form>\n\n\t\t<form action=\"/login-with-action-element\">\n\t\t\t<label for=\"user\">user:</label>\n\t\t\t<input type=\"text\" id=\"user\" name=\"user\"><br><br>\n\t\t\t<label for=\"password\">password:</label>\n\t\t\t<input type=\"password\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"submit\" name=\"action\" value=\"login\">\n\t\t</form>\n\t\t<!--/login-with-non-url-action-attribute-->\n\t\t<form>\n\t\t\t<label for=\"user\">user:</label>\n\t\t\t<input type=\"text\" id=\"user\" name=\"user\"><br><br>\n\t\t\t<label for=\"password\">password:</label>\n\t\t\t<input type=\"password\" id=\"password\" name=\"password\"><br><br>\n            <input type=\"submit\" name=\"action\" value=\"login\">\n\t\t</form>\n\n         <form id=\"jsonForm\">\n        <label>param1:</label>\n        <input name=\"param1\" id=\"param1\">\n        <button type=\"submit\">Submit</button>\n    </form>\n\n    <pre id=\"response\"></pre>\n\n    <script>\n    const form = document.getElementById(\"jsonForm\");\n    form.addEventListener(\"submit\", async (e) => {\n        e.preventDefault(); // prevent normal form submission\n\n        const param1 = document.getElementById(\"param1\").value;\n\n        // send JSON using fetch\n        const res = await fetch(\"/reflected\", {\n            method: \"POST\",\n            headers: { \"Content-Type\": \"application/json\" },\n            body: JSON.stringify({ param1 })\n        });\n\n        const text = await res.text();\n        document.getElementById(\"response\").textContent = text;\n    });\n    </script>\n\n<!-- External JS files -->\n<script src=\"/static/notexecuted-script.js\"></script>\n<script src=\"/static/executed-script.js\"></script>\n<script src=\"/static/main.js\"></script>\n\n<!-- jQuery and Axios -->\n<script src=\"https://code.jquery.com/jquery-3.6.0.min.js\"></script>\n<script src=\"https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js\"></script>\n\n<script>\nconst base = 'http://localhost:3000/test';\n\n\n// ===== Requests that actually run =====\nfunction runAllRequests() {\n    // --- Fetch ---\n    fetch(base + '/fetch1');\n    fetch(base + '/fetch2', { method: 'POST', body: JSON.stringify({a:1}) });\n    \n    fetch('http://localhost:1111/fetchasdf', { method: 'POST', body: {\"a\":1} });\n\n    // --- XHR ---\n    const xhr1 = new XMLHttpRequest();\n    xhr1.open('GET', base + '/xhr1');\n    xhr1.send();\n\n    const xhr2 = new XMLHttpRequest();\n    xhr2.open('POST', base + '/xhr2');\n    xhr2.send('param=value');\n\n    // --- Formdata ---\n    var form = document.getElementById('myForm'); // give the form an ID\n    var xhr  = new XMLHttpRequest();              // create XMLHttpRequest\n    var data = new FormData(form);                // create formData object\n\n\n    xhr.onload = function() {\n        console.log(this.responseText); // whatever the server returns\n    }\n\n    xhr.open(\"post\", form.action);      // open connection\n    xhr.send(data);                     // send data\n\n\n    $.ajax({ url: base + '/jqueryparam1?asdf=1', method: 'GET' });\n\n    $.ajax({ url: base + '/jqueryparam2?a=1&b=2&c=3', method: 'GET' });\n\n    // --- jQuery ---\n    if (window.jQuery) {\n        $.ajax({ url: base + '/jquery1', method: 'GET' });\n        $.ajax({ url: base + '/jquery2', method: 'POST', data: {a:1} });\n    }\n\n    // --- Axios ---\n    if (window.axios) {\n        axios.get(base + '/axios1');\n        axios.post(base + '/axios2', {x:42});\n    }\n\n    // --- sendBeacon ---\n    navigator.sendBeacon(base + '/beacon', 'beacon-data');\n\n    // --- Image load ---\n    const img = new Image();\n    img.src = base + '/image-load';\n\n    // --- Script load ---\n    const script = document.createElement('script');\n    script.src = base + '/dynamic-script.js';\n    document.body.appendChild(script);\n\n    // --- Iframe ---\n    const iframe = document.createElement('iframe');\n    iframe.src = base + '/iframe-content';\n    document.body.appendChild(iframe);\n\n    // --- Link stylesheet ---\n    const link = document.createElement('link');\n    link.rel = 'stylesheet';\n    link.href = base + '/style.css';\n    document.head.appendChild(link);\n\n    console.log(\"All requests triggered\");\n}\n\nrunAllRequests();\n\n// ===== Functions containing requests that are never executed =====\nfunction notexecuted(){\n    const b = 'http://localhost:3000/notexec';\n\n    fetch(b + '/fetch');\n    const x = new XMLHttpRequest();\n    x.open('GET', b + '/xhr');\n    x.send();\n\n    if (window.jQuery) {\n        $.ajax({ url: b + '/jquery', method: 'GET' });\n    }\n\n    if (window.axios) {\n        axios.get(b + '/axios');\n    }\n\n    navigator.sendBeacon(b + '/beacon', 'data');\n}\n\nfunction deepUnused() {\n    function innerUnused() {\n        fetch('http://localhost:3000/deep-inner');\n    }\n}\n\nfunction innerUnused() {\n    fetch('http://localhost:3000/deep-inner?a=1');\n    fetch('http://localhost:3000/deep-inner?a=1123123&b=ASIOFHSDF&c=1');\n    fetch('http://localhost:3000/deep-inner123?cd=1337&b=asdfsdf&c=1');\n}\n\n// ===== Inline unused variable with request =====\nconst unusedVar = () => {\n    axios.post('http://localhost:3000/unused-var', {test: true});\n};\n\n</script>\n\n</body>\n</html>\n"
